{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "scaffold",
  "title": "Scaffold",
  "description": "Creates project scaffolds and then opens them up in VSCode",
  "icon": "scaffold-icon.png",
  "author": "JoshuaRileyDev",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "newProject",
      "title": "New Project",
      "description": "",
      "mode": "view"
    },
    {
      "name": "createTemplate",
      "title": "Create Template",
      "description": "",
      "mode": "view"
    },
    {
      "name": "manageTemplates",
      "title": "Manage Templates",
      "description": "",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.65.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}